{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch with inspect",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "dev"],
			"console": "integratedTerminal",
			"skipFiles": ["<node_internals>/**"]
		},
		{
			"name": "Attach to running process",
			"type": "node",
			"request": "attach",
			"restart": true,
			"timeout": 10000
		}
	]
}
